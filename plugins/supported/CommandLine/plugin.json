{
    "depends": [ "Canon" ],
    "provides": [
        {
            "ep": "factory",
            "name": "commandline",
            "action": "value",
            "pointer": "views/cli#cliInputView"
        },
        {
            "ep": "factory",
            "name": "tokenizer",
            "action": "value",
            "pointer": "tokenizer#tokenize"
        },
        {
            "ep": "factory",
            "name": "history",
            "action": "create",
            "pointer": "history#InMemoryHistory"
        },
        {
            "ep": "command",
            "name": "help",
            "takes": [ "search" ],
            "description": "Get help on the available commands. Add an optional search string to narrow the output.",
            "completeText": "optionally, narrow down the search",
            "pointer": "commands#helpCommand"
        },
        {
            "ep": "command",
            "name": "alias",
            "takes": [ "alias", "command" ],
            "description": "define and show aliases for commands",
            "completeText": "optionally, add your alias name, and then the command name",
            "usage": "",
            "pointer": "commands#aliasCommand"
        },
        {
            "ep": "command",
            "name": "history",
            "description": "Show history of the commands",
            "pointer": "commands#historyCommand"
        },
        {
            "ep": "choice",
            "name": "historytimemode",
            "description": "Display number|date|none next to each historical instruction",
            "type": "text",
            "defaultValue": "time"
        },
        {
            "ep": "command",
            "name": "eval",
            "takes": [ "js-code" ],
            "description": "evals given js code and show the result",
            "completeText": "evals given js code and show the result",
            "hidden": true,
            "pointer": "basiccommands#evalCommand"
        },
        {
            "ep": "command",
            "name": "version",
            "takes": [ "command" ],
            "description": "show the version for Bespin or a command",
            "completeText": "optionally, a command name",
            "hidden": true,
            "pointer": "basiccommands#versionCommand"
        },
        {
            "ep": "command",
            "name": "bespin",
            "description": "has",
            "hidden": true,
            "pointer": "basiccommands#bespinCommand"
        }
    ]
}
