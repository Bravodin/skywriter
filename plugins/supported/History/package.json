{
    "description": "Command line history",
    "dependencies": {
        "Canon": "0.0"
    },
    "provides":
    [
        {
            "ep": "factory",
            "name": "history",
            "action": "create",
            "pointer": "history#InMemoryHistory"
        },
        {
            "ep": "command",
            "name": "historyPrevious",
            "predicates": { "isCommandLine": true, "isKeyUp": true },
            "key": "up",
            "pointer": "commands#historyPreviousCommand"
        },
        {
            "ep": "command",
            "name": "historyNext",
            "predicates": { "isCommandLine": true, "isKeyUp": true },
            "key": "down",
            "pointer": "commands#historyNextCommand"
        },
        {
            "ep": "command",
            "name": "history",
            "description": "Show history of the commands",
            "pointer": "commands#historyCommand",
            "params": []
        }
    ]
}
