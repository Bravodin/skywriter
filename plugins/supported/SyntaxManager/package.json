{
    "description": "Provides syntax highlighting services for the editor",
    "dependencies": {
        "DelegateSupport": "0.0",
        "RangeUtils": "0.0"
    },
    "provides":
    [
        {
            "ep": "extensionhandler",
            "name": "syntax",
            "register": "controllers/syntaxdirectory#discoveredNewSyntax"
        },
        {
            "ep": "extensionpoint",
            "name": "fileextension",
            "indexOn": "name"
        }
    ]
}

